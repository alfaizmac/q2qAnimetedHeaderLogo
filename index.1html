<script>
  // defensive: wait for DOM, check element exists
  document.addEventListener('DOMContentLoaded', () => {
    const btn = document.querySelector('.hover-trigger-button');
    if (!btn) {
      console.error('q2qtechnologies element not found');
      return;
    }

    const root = document.documentElement;
    const q2qbgleft = document.querySelector('.q2q-bg-left');
    const q2qbgcenter = document.querySelector('.q2q-bg-center');
    const technologies = document.querySelectorAll('.technologies');
    const q2q = document.querySelectorAll('.q2q');
    const logorow = document.querySelector('.logo-row');    

    const q2qtechnologies = document.querySelector('.q2qtechnologies');


    const profileservice = document.querySelector('.profileservice');
    const profileserviceBGleft = document.querySelector('.profileservice-bg-left');
    const profileserviceBGcenter = document.querySelector('.profileservice-bg-center');
    const profile= document.querySelectorAll('.profile');
    const service = document.querySelectorAll('.service');


    

    btn.addEventListener('mouseenter', () => {

        profileserviceBGleft.style.display = 'block';
        profileserviceBGcenter.style.display = 'block';
        profile.forEach(el => {
            el.style.display = 'block';
        });
        service.forEach(el => {
            el.style.display = 'block';
        });

       root.style.setProperty('--profileservice-offset',' 1200px');

    });

    btn.addEventListener('mouseleave', () => {

    });

    function triggerAnimation() {
    
      root.style.setProperty('--shared-anchor-x','320px');

    q2qbgleft.style.transform = 'scaleX(1) translateX(310px)';
    q2qbgcenter.style.transform = 'scaleX(1) translateX(310px)';

      q2q.forEach(el => {
        el.style.transition = 'fill 1s ease-in-out';
        el.style.fill = '#660361';
      });

      setTimeout(() => {
        technologies.forEach(el => el.classList.add('show'));
      }, 600);
    }

    window.onload = function() {
        setTimeout(() => {
            triggerAnimation();
    }, 1000);
    };

  });

</script>
    
